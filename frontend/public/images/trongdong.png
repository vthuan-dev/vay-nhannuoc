  <script language="javascript" type="text/javascript">
    function showLangPopup()
    {
      document.getElementById('langList').style.visibility = 'visible';
      return false;
    }

    function hideLanguagePopup()
    {
      document.getElementById('langList').style.visibility = 'hidden';
      return false;
    }

    function setLangCookie(langName, cookiePath)
    {
      /**
       * the same constant is also defined in LocaleUtils
       * which is used when creating cookie using java server code.
       * any change to the cookieName has to be kept in synch.
       */
      var cookieName = 'ORA_WEBCENTERAPP_USER_preferredLang';
      var today = new Date();
      var expire = new Date();
      expire.setTime(today.getTime() + 1000 * 60 * 60 * 24 * 365);
      var cookieStr = cookieName + "=" + escape(langName) + "; expires=" + expire.toGMTString() + "; path=" + cookiePath;
      document.cookie = cookieStr;
      return false;
    }

    function setLanguage(language)
    {
      setLangCookie(language, '/webcenter');
      hideLanguagePopup();
      window.location.reload();
      return false;
    }
  </script>




